{
  "id": null,
  "title": "OpenTelemetry Collector Metrics",
  "tags": ["otel", "prometheus"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Prometheus Targets",
      "type": "stat",
      "targets": [
        {
          "expr": "up",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Scrape Duration",
      "type": "timeseries",
      "targets": [
        {
          "expr": "prometheus_target_scrape_duration_seconds",
          "legendFormat": "{{job}}"
        }
      ],
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
    },
    {
      "id": 3,
      "title": "HTTP Requests",
      "type": "timeseries",
      "targets": [
        {
          "expr": "rate(promhttp_metric_handler_requests_total[5m])",
          "legendFormat": "{{code}}"
        }
      ],
      "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
    }
  ],
  "time": {"from": "now-1h", "to": "now"},
  "refresh": "5s"
}
